{
  "extends": "tslint:latest",
  "rules": {
    "max-line-length": [true, 120],
    "curly": false,
    "one-line": false,
    "no-trailing-whitespace": false,
    "comment-format": false,
    "interface-name": false,
    "arrow-parens": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "radix": false,
    "no-consecutive-blank-lines": false,
    "no-console": [
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ]
  },
  "jsRules": {
    "max-line-length": [true, 120]
  }
}
